{
  "name": "@kratapps/sfdx-plugin",
  "description": "Kratapps SFDX plugin",
  "version": "0.1.0",
  "author": "Kratapps",
  "bugs": "https://github.com/kratapps/kratapps-sfdx-plugin/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^2",
    "@salesforce/core": "^2",
    "fs-extra": "10.0.0",
    "jsforce": "^1",
    "node-fetch": "2.6.1",
    "rimraf": "3.0.2",
    "tmp": "0.2.1",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.4.1",
    "@salesforce/ts-sinon": "^1.3.18",
    "@types/chai": "^4",
    "@types/jsforce": "^1.9.30",
    "@types/fs-extra": "^9.0.11",
    "@types/mocha": "^5",
    "@types/node": "^15.12.4",
    "@types/node-fetch": "^2.5.10",
    "@types/rimraf": "^3.0.0",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "tslint": "^5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kratapps/kratapps-sfdx-plugin",
  "keywords": [
    "sfdx-plugin",
    "kratapps",
    "sfdx",
    "plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "kratapps:remote": {
        "description": "Commands related to remote services, e.g. github"
      },
      "kratapps:remote:source": {
        "description": "Commands related to remote source, e.g. github"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}

{
  "name": "@kratapps/sfdx-plugin",
  "description": "Kratapps SFDX plugin",
  "version": "0.2.2",
  "author": "Kratapps",
  "bugs": "https://github.com/kratapps/kratapps-sfdx-plugin/issues",
  "dependencies": {
    "@oclif/command": "1.8.25",
    "@oclif/config": "1.18.8",
    "@oclif/errors": "1.3.6",
    "@salesforce/command": "5.3.8",
    "@salesforce/core": "3.36.1",
    "@salesforce/plugin-deploy-retrieve": "^1.9.0",
    "@salesforce/sf-plugins-core": "^2.4.2",
    "chalk": "^5.2.0",
    "fs-extra": "10.0.0",
    "jsforce": "^1",
    "node-fetch": "^2.6.11",
    "rimraf": "3.0.2",
    "tmp": "0.2.1",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.4.1",
    "@salesforce/ts-sinon": "^1.3.18",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.11",
    "@types/jsforce": "^1.9.30",
    "@types/mocha": "^5",
    "@types/node": "^15.12.4",
    "@types/node-fetch": "^2.5.10",
    "@types/rimraf": "^3.0.0",
    "@types/tmp": "^0.2.3",
    "chai": "^4",
    "globby": "^13.1.4",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^4.9.5",
    "wireit": "^0.9.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kratapps/kratapps-sfdx-plugin",
  "keywords": [
    "sfdx-plugin",
    "kratapps",
    "sfdx",
    "plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "kratapps remote deploy start": {
        "description": "Deploy remote code, for example from GitHub."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "wireit",
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "compile"
      ]
    },
    "compile": {
      "command": "tsc -p . --pretty --incremental",
      "files": [
        "src/**/*.ts",
        "**/tsconfig.json",
        "messages/**"
      ],
      "output": [
        "lib/**",
        "*.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    }
  }
}
